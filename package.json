{
  "name": "menno-webcomponents",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && npm run test:puppeteer",
    "start": "npm run build && npm run watch & http-server components/dist2 -p 3000",
    "test:server": "http-server components -p 3001",
    "test:puppeteer": "npm run test:server & mocha components/tests/puppeteer/*/*.js",
    "posttest:puppeteer": "kill $(lsof -t -i:3001)",
    "build": "node build/index.js",
    "build:table": "npm run clean && webpack --config webpack.config.js",
    "clean": "rm -r components/dist2",
    "watch": "node build/watch.js",
    "start:dev": "webpack-dev-server",
    "deploy": "./deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manvel/webcomponents.git"
  },
