  },
  "homepage": "https://github.com/Manvel/webcomponents#readme",
  "dependencies": {
    "http-server": "^0.11.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.4",
    "mocha": "^6.1.4",
    "puppeteer": "^1.17.0",
    "raw-loader": "^3.1.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
